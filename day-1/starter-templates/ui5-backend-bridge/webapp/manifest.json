{
  "_version": "1.32.0",
  "sap.app": {
    "id": "workshop.ui5.bridge",
    "type": "application",
    "title": "UI5 to Backend Bridge",
    "description": "Transition project from UI5 frontend to backend services"
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.98.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.json.JSONModel"
      },
      "odata": {
        "dataSource": "odataService",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "defaultCountMode": "Request"
        }
      }
    }
  },
  "sap.app/dataSources": {
    "mainService": {
      "uri": "model/mockData.json",
      "type": "JSON"
    },
    "odataService": {
      "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
      "type": "OData",
      "settings": {
        "annotations": [],
        "localUri": "",
        "odataVersion": "2.0"
      }
    }
  }
}
