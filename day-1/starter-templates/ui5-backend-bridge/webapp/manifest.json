{
  "_version": "1.32.0",
  "sap.app": {
    "id": "workshop.ui5.bridge",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "UI5 to Backend Bridge",
    "description": "Transition project from UI5 frontend to backend services",
    "dataSources": {
      "mainService": {
        "uri": "model/mockData.json",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task"
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.98.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.json.JSONModel"
      }
    }
  }
}
  